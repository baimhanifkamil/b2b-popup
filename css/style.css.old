/***** Buttons  *****/
.button {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 5px;
  margin-bottom: 5px;
  border-radius: 4px;
  box-shadow: 2px 2px 5px 0 rgba(99, 99, 99, 0.15);
  font-family: "FuturaStd-Heavy";
  font-size: 15px;
}
.button.primary {
  position: relative;
  width: 200px;
  height: 45px;
  color: #FFFFFF !important;
  border: 2px solid transparent;
  box-shadow: inset 0 -4px 0 0 #86B882;
  background-color: #96CD91;
  background-clip: padding-box;
  padding: 2px;
}
.button.primary::after {
  position: absolute;
  top: -2px;
  bottom: -2px;
  left: -2px;
  right: -2px;
  background: linear-gradient(#ACE2A7, #86B882);
  border-radius: 4px;
  content: '';
  z-index: -1;
}
.button.secondary {
  width: 200px;
  height: 45px;
}

.button-popup-call {
  font-family: "FuturaStd-Heavy";
  font-size: 0.9em;
  background: #96cd91;
  width: 372px;
  color: white;
  display: block;
  padding: 1em;
  border-radius: 5em;
  text-transform: uppercase;
  text-align: center;
  text-decoration: none;
  letter-spacing: 0.15em;
  box-shadow: 0 15px 20px -10px rgba(0, 0, 0, 0.3);
  transition: color 0.6s, box-shadow 0.3s, transform 0.3s;
}
.button-popup-call:hover {
  box-shadow: 0 3px 5px -5px rgba(0, 0, 0, 0.3);
  transform: translateY(-1px);
  color: #e6e6e6;
}

.button-popup-schedule {
  font-family: "FuturaStd-Heavy";
  margin-top: 10px;
  font-size: 0.9em;
  background: #96cd91;
  width: 372px;
  color: white;
  display: block;
  padding: 1em;
  border-radius: 5em;
  text-transform: uppercase;
  text-align: center;
  text-decoration: none;
  letter-spacing: 0.15em;
  box-shadow: 0 15px 20px -10px rgba(0, 0, 0, 0.3);
  transition: color 0.6s, box-shadow 0.3s, transform 0.3s;
}
.button-popup-schedule:hover {
  box-shadow: 0 3px 5px -5px rgba(0, 0, 0, 0.3);
  transform: translateY(-1px);
  color: #e6e6e6;
}

